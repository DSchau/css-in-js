{
  "name": "css-in-js",
  "version": "1.0.0",
  "description": "ReactJS Powered Presentation Framework",
  "main": "dist/bundle.js",
  "scripts": {
    "precommit": "lint-staged",
    "prebuild": "npm run clean && node scripts/images.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js",
    "clean": "rimraf dist",
    "format": "prettier \"webpack.*.js\" \"src/**/*.js\"  \"themes/**/*.js\" --write --single-quote",
    "images": "node scripts/images.js",
    "lint": "eslint --ext .js,.jsx .",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --port 3000 --content-base dist --no-info --config webpack.config.development.js",
    "test": "echo \"Kinda hard to test a presentation, huh?\""
  },
  "author": "Dustin Schau <dustinschau@gmail.com> (https://dustinschau.com)",
  "license": "MIT",
  "dependencies": {
    "@vx/axis": "^0.0.136",
    "@vx/curve": "^0.0.136",
    "@vx/gradient": "^0.0.136",
    "@vx/group": "^0.0.136",
    "@vx/legend": "^0.0.136",
    "@vx/mock-data": "^0.0.136",
    "@vx/scale": "^0.0.136",
    "@vx/shape": "^0.0.136",
    "@vx/text": "^0.0.136",
    "d3-array": "^1.2.0",
    "emojione": "2.2.5",
    "es6-promise": "^4.1.1",
    "isomorphic-fetch": "^2.2.1",
    "normalize.css": "3.0.3",
    "polished": "^1.6.2",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-icons": "^2.2.5",
    "spectacle": "^2.2.0",
    "spectacle-code-slide": "^0.4.0",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-plugin-transform-decorators-legacy": "^1.2.0",
    "babel-plugin-transform-react-constant-elements": "^6.4.0",
    "babel-plugin-transform-react-inline-elements": "^6.4.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.18.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.0",
    "eslint": "^3.8.0",
    "eslint-config-formidable": "^2.0.1",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-react": "^6.4.1",
    "express": "^4.13.3",
    "file-loader": "^0.9.0",
    "gh-pages": "^1.0.0",
    "glob": "^7.1.2",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "~2.28.0",
    "husky": "^0.14.3",
    "is-buffer": "^1.1.1",
    "lint-staged": "^4.1.0",
    "markdown-loader": "^0.1.7",
    "node-libs-browser": "^0.5.3",
    "object-assign": "~4.1.1",
    "prettier": "^1.5.3",
    "raw-loader": "^0.5.1",
    "react-svg-loader": "^1.1.1",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.0",
    "rimraf": "^2.4.4",
    "sharp": "^0.18.2",
    "style-loader": "^0.13.0",
    "surge": "latest",
    "svg-react-loader": "^0.4.5",
    "url-loader": "^0.5.6",
    "webpack": "~2.4.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "~2.4.5",
    "webpack-hot-middleware": "^2.13.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run format",
      "git add"
    ]
  }
}
